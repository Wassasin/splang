[Int] function (Int n, Bool b, [Int] l)
{
    print(n);
    print(b);
    print(l);
}

Void main ()
{
    [Int] list = 1:2:3:[];

    // No arguments
    function();
    // One argument
    function(5);
    // Two arguments
    function(10, True);
    // Three arguments
    function(15, False, list);
}

